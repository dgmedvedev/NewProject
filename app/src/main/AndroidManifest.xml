<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="by.itacademy.project">

    <application
            android:name="by.itacademy.app.App"
            android:allowBackup="true"
            android:label="@string/app_name"
            android:icon="@mipmap/ic_launcher"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-5588713578108649~8946020371"/>

        <activity android:name="by.itacademy.project.NoteDetailsActivity"/>
        <activity android:name="by.itacademy.project.NoteEditActivity"/>

        <activity android:name="by.itacademy.project.ProjectActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

    </application>
</manifest>